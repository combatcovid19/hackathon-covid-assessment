	<parent>

		<groupId>org.springframework.boot</groupId>

		<artifactId>spring-boot-starter-parent</artifactId>

		<version>2.2.4.RELEASE</version>

		<relativePath /> <!-- lookup parent from repository -->

	</parent>

	<properties>

		<service.name>Covid - 19 Hackathon Assessment Service</service.name>



		<!-- Java Version -->

		<java.version>11</java.version>



		<!-- Maven version as of 1/15/2019 -->

		<maven-compiler.version>3.8.0</maven-compiler.version>



		<!-- Spring Boot/Cloud version as of 1/15/2019. Please upgrade as necessary -->

		<spring.boot.version>2.2.4.RELEASE</spring.boot.version>

		<spring.cloud.version>Finchley.RELEASE</spring.cloud.version>



		<lombok.version>1.18.10</lombok.version>





		<!-- API Documentation using Swagger -->

		<springfox.swagger.version>2.9.2</springfox.swagger.version>

		<google.guava.version>28.2-jre</google.guava.version>



		<!-- JAXB used to generate Object/Json DTOs from xsd schema file -->

		<jaxb.version>1.6</jaxb.version>

		<jaxb.runtime.version>0.6.4</jaxb.runtime.version>



		<!-- Testing framework -->

		<junit.version>4.12</junit.version>

		<mockito.version>1.9.5</mockito.version>

		<moco.version>0.11.0</moco.version>

		<easymock.version>2.4</easymock.version>

		<json.version>20180130</json.version>

		<objenesis.version>2.5</objenesis.version>







		<!-- Code Quality modules -->

		<maven-check-style-plugin-version>2.17

		</maven-check-style-plugin-version>

		<maven-jxr-plugin-version>2.5</maven-jxr-plugin-version>



		<!-- SonarQube properties -->

		<jacoco.version>0.8.5</jacoco.version>

		<sonar.projectKey>GROUP-CONNECT-DOCUMENT-SERVICE</sonar.projectKey>

		<sonar.projectName>Group connect document service</sonar.projectName>

		<sonar.language>java</sonar.language>

		<sonar.sourceEncoding>UTF-8</sonar.sourceEncoding>

		<sonar.java.coveragePlugin>jacoco</sonar.java.coveragePlugin>

		<sonar-jacoco-listeners.version>1.4</sonar-jacoco-listeners.version>

		<sonar.coverage.jacoco.xmlReportPath>target/site/jacoco/jacoco.xml

		</sonar.coverage.jacoco.xmlReportPath>

		<!-- Group Invoice Billing Domain Library -->

		<group-invoice-billing-domain-library-version>2.0.10

		</group-invoice-billing-domain-library-version>

		<!-- Services Common Library -->

		<services-common-library-version>1.0.3

		</services-common-library-version>

		<!-- Logger versions -->

		<log4j2.version>2.11.1</log4j2.version>

		<slf4j.version>1.7.25</slf4j.version>

		<sqlserver.version>4.0</sqlserver.version>

		<jackson.version>2.10.2</jackson.version>

		<mockito.version>1.9.5</mockito.version>

		<joda.datetime.version>2.10.5</joda.datetime.version>



	</properties>



	<dependencyManagement>

		<dependencies>

			<dependency>

